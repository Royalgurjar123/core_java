package Exceptionhandling;

public class TestLoginExpection   {
public static void main(String[] args)throws LoginExpection {
	try {
		String name = "rahul";
		if (name.equals("raghu")) {
    System.out.println("vailid user---!!!!");
		}	else {
    	
			throw new LoginExpection();
		
	}
	
		
	} catch (Exception e) {
		System.out.println(e);
		
	}
	
}

}
